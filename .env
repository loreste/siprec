# Minimal test configuration
APP_ENV=development
LOG_LEVEL=trace

# Network configuration
PORTS=5060,5061
HTTP_PORT=8080
RTP_PORT_MIN=16384
RTP_PORT_MAX=32768

# TLS Configuration for large messages
ENABLE_TLS=true
TLS_CERT_PATH=./certs/cert.pem
TLS_KEY_PATH=./certs/key.pem
TLS_PORT=5065

# STT configuration
STT_VENDORS=mock
STT_DEFAULT_LANGUAGE=en-US
STT_BUFFER_SIZE=2048

# Disable encryption for testing
ENABLE_RECORDING_ENCRYPTION=false
ENABLE_METADATA_ENCRYPTION=false

# Session configuration
SESSION_CLEANUP_INTERVAL=30s
SESSION_MAX_IDLE_TIME=5m

# Performance
MAX_CONCURRENT_CALLS=100
WORKER_POOL_SIZE=50